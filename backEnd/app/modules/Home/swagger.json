{
  "paths": {
    "/homepage/recent-job": {
      "get": {
        "tags": ["Home"],
        "description": "recently added 9 jobs , 6 key words",
        "parameters": [
          {
            "name": "searchText",
            "in": "query",
            "description": "search by using searchText",
            "schema": {
              "type": "String",
              "example": "UI/UX"
            }
          },
          {
            "name": "location",
            "in": "query",
            "description": "filter by location",
            "schema": {
              "type": "String",
              "example": "641804483404984d4de40f06"
            }
          },
          {
            "name": "jobType",
            "in": "query",
            "description": "filter with job type",
            "schema": {
              "type": "String",
              "example": "641804483404984d4de40f06"
            }
          },
          {
            "name": "sortBy",
            "in": "query",
            "description": "sort the list",
            "schema": {
              "type": "String",
              "example": "latest"
            }
          },
          {
            "name": "page",
            "in": "query",
            "description": "page number",
            "schema": {
              "type": "Number",
              "example": 1
            }
          },
          {
            "name": "pageSize",
            "in": "query",
            "description": "get number of results per page",
            "schema": {
              "type": "Number",
              "example": 10
            }
          },
          {
            "name": "status",
            "in": "query",
            "description": "get published jobs only",
            "schema": {
              "type": "String",
              "example": "Published"
            }
          },
          {
            "name": "experience",
            "in": "query",
            "description": "get jobs based on experience matched jobs only",
            "schema": {
              "type": "String",
              "example": "value from admin master experience list"
            }
          },
          {
            "name": "industry",
            "in": "query",
            "description": "get jobs based on industry matched jobs only",
            "schema": {
              "type": "String",
              "example": "value from admin master industries list"
            }
          },
          {
            "name": "payRate",
            "in": "query",
            "description": "get jobs based on payRate from and to matched jobs only",
            "schema": {
              "type": "Number",
              "example": "value from admin master payrates list (20-50)"
            }
          },
          {
            "name": "keywords",
            "in": "query",
            "description": "if you want to get keywords array pass true",
            "schema": {
              "type": "Boolean",
              "example": "true"
            }
          }
        ],
        "security": {
          "basicAuth": []
        },
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "recently added 9 jobs , total posted jobs , 6 key words",
            "schema": {
              "$ref": "#/definitions/home-recent-job-response"
            }
          }
        }
      }
    },
