        "pagesize": {
          "type": "number",
          "example": "10",
          "required": true
        },
        "filter": {
          "type": "object",
          "example": [
            {
              "countryName": [
                ""
              ]
            }
          ],
          "required": true
        },
        "sort": {
          "type": "object",
          "example": {
            "countryName": 1
          },
          "required": true
        },
        "searchText": {
          "type": "string",
          "example": "India",
          "required": true
        }
      }
    },
    "CountryListingRes": {
      "properties": {
